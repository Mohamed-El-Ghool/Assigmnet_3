
package assignment3_1;
public class BaseBall extends Ball{
    public BaseBall(String brandName) {
        super(brandName);
    }

    @Override
    public void toss() {
        System.out.println("toss method has been implemented by BaseBall class");
    }

    @Override
    public void bonuce() {
        System.out.println("bonuce method hase been overrided by Baseball class");
    }
    
    
}
